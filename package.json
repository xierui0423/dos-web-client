{
  "name": "dos-web-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/helper/test_helper.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "lint": "eslint --ext .jsx,.js --fix src",
    "build": "webpack",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xierui0423/dos-web-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xierui0423/dos-web-client/issues"
  },
  "homepage": "https://github.com/xierui0423/dos-web-client#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.10.3",
    "jsdom": "^9.12.0",
    "mocha": "^3.5.0",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "material-ui": "^0.16.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.8",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.3",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^0.9.0",
    "redux-form": "^6.8.0",
    "redux-form-material-ui": "^4.2.0",
    "redux-immutable": "^3.1.0",
    "redux-saga": "^0.14.8",
    "socket.io-client": "^1.7.4",
    "validator": "^6.3.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "$": true
    },
    "extends": [
      "airbnb"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "new-cap": [
        0
      ],
      "linebreak-style": [
        2,
        "windows"
      ],
      "react/no-unused-prop-types": [
        0
      ],
      "react/jsx-indent-props": [
        2,
        4
      ],
      "import/prefer-default-export": [
        0
      ],
      "react/prop-types": [
        0
      ],
      "no-confusing-arrow": [
        0
      ],
      "class-methods-use-this": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "no-console": [
        0
      ]
    }
  }
}
