{
  "name": "dos-web-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/helper/test_helper.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "build": "webpack",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xierui0423/dos-web-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xierui0423/dos-web-client/issues"
  },
  "homepage": "https://github.com/xierui0423/dos-web-client#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "material-ui": "^0.16.6",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^0.9.0",
    "redux-form": "^6.4.3",
    "redux-form-material-ui": "^4.1.2",
    "redux-immutable": "^3.0.9",
    "redux-saga": "^0.14.0",
    "validator": "^6.2.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "$": true
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "react/jsx-indent": [
        2,
        4
      ],
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ],
      "linebreak-style": [
        2,
        "windows"
      ],
      "react/no-unused-prop-types": [
        0
      ],
      "react/jsx-indent-props": [
        2,
        4
      ],
      "import/prefer-default-export": [
        0
      ],
      "react/prop-types": [
        0
      ],
      "no-confusing-arrow": [
        0
      ],
      "class-methods-use-this": [
        0
      ]
    ,
      "new-cap": [
        0
      ]
    }
  }
}
